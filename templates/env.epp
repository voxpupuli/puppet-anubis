<%- |
  Stdlib::HTTPUrl $target,
  Optional[Stdlib::Absolutepath] $bot_policies_file,
  Hash[String,String] $settings,
  | -%>
TARGET=<%= $target %>
<% if $bot_policies_file { %>
POLICY_FNAME=<%= $bot_policies_file %>
<% } %>
<% $settings.each |$setting,$value| { %>
<%= $setting %>=<%= $value %>
<% } %>
